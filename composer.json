{
    "name": "programmerzamannow/belajar-php-unit-test",
    "description": "Belajar PHP Unit Test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Programmerzamannow\\BelajarPhpUnitTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programmerzamannow\\BelajarPhpUnitTest\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ozankaze"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.2.2"
    },
    "scripts": {
        "tests" : "phpunit --configuration phpunit.xml",
        "tests:product" : "phpunit --configuration phpunit.xml --testsuite product",
        "tests:counter" : "phpunit --configuration phpunit.xml --testsuite counter"
    }
}
